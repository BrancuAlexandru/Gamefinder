{
  "name": "Gamefinder",
  "description": "Finds what game a youtube video features.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "favicon.png"
  },
  "content_scripts": [{
    "matches": ["https://*.youtube.com/*"],
    "js": ["contentScripts.js"],
    "run_at": "document_end"
  }],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["https://*.youtube.com/*"]
}